{
  "name": "sketch-lazy-sorter",
  "description": "Sort Layers alphabetically in the Layers panel.",
  "homepage": "https://github.com/lazybearcreations/sketch-lazy-sorter",
  "bugs": {
    "url": "https://github.com/lazybearcreations/sketch-lazy-sorter/issues"
  },
  "keywords": [
    "sketch",
    "sketchapp",
    "plugin",
    "layers",
    "layer sort",
    "layer sorter",
    "lazybear creations"
  ],
  "license": "MIT",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "sketch-lazy-sorter",
    "manifest": "src/manifest.json",
    "main": "sketch-lazy-sorter.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "author": "Adam Bruzon <adam@lbc.email>",
  "repository": {
    "type": "git",
    "url": "git+git@github.com:lazybearcreations/sketch-lazy-sorter.git"
  },
}
